    android:layout_height="match_parent" >

    <EditText
        android:id="@+id/et_add_company_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="公司名称" >

        <requestFocus />
    </EditText>

    <EditText
        android:id="@+id/et_add_company_note"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="备注" />

</TableLayout>
